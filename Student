
public class Student implements Comparable{
	
	private String first;
	private String last;
	
	public Student(String f, String l)
	{
		first = f;
		last = l;
	}
	
	public String getFirst()
	{
		return first;
	}
	
	public String getLast()
	{
		 return last;
	}
	
	
	public boolean equals(Object other) //NO LONGER NECESSARY????
	{
		if (other instanceof Student1)
		{
			Student1 o = (Student1)other;
			if (this.hashCode() == o.hashCode()){
				return true;
			}
		}
		return false;
	}
	
	public int compareTo(Object other)
	{
		Student comparing;
		try{
			comparing = (Student)other;
		} catch (ClassCastException e){
			System.out.println("You can't try to add a non-student to this tree");
			return -5;
		}
		String thisName = last+first;
		String otherName = comparing.getLast() + comparing.getFirst();
		return (thisName.compareTo(otherName));
		
	}
	
	public String toString(){
		return (first + " " + last + " ");
	}

}
